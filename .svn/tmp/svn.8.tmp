<?xml version="1.0" encoding="UTF-8"?>
<suite thread-count="5" verbose="1" name="RegressionScript"
	annotations="JDK">
	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
		<listener class-name="com.jbilling.framework.testrails.TestRailsListener" />
	</listeners>
	<test name="VerifyStatusofPreExistingandNewOrder" junit="false"
		preserve-order="true">
		<classes>
			<class name="com.jbilling.test.VerifyStatusofPreExistingandNewOrder">
				<methods>
					<include name="VerifyStatusofPreExistingandNewOrder"></include>
					
				</methods>
			</class>
		</classes>
