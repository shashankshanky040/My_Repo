<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite thread-count="5" verbose="1" name="RegressionScript"
	annotations="JDK">
	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
		<listener class-name="com.jbilling.framework.testrails.TestRailsListener" />
	</listeners>
	<test name="VerifyUserAbleToPrepareDataForAllSections" junit="false"
		preserve-order="true">
		<classes>
			<class name="com.jbilling.test.VerifyUserAbleToPrepareDataForAllSections">
				<methods>
					<include name="dataPreprationForAllSections"></include>
				</methods>
			</class>
		</classes>
	</test>
</suite>